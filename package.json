{
  "name": "vanillamod-transpiler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "lint": "eslint components/**/*.js && eslint src/**/*.js",
    "pretty": "prettier --write components/ && prettier --write src/",
    "udeploy": "GIT_USER=$(grep GIT_USER .env | cut -d '=' -f2) docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.0",
    "@docusaurus/preset-classic": "^2.0.0-beta.0",
    "@mdx-js/react": "^1.6.21",
    "@monaco-editor/react": "^4.0.9",
    "bootstrap": "4.6.0",
    "bootswatch": "^5.0.1",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.1.11",
    "docusaurus2-dotenv": "^1.4.0",
    "esprima": "^4.0.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.5.0",
    "monaco-editor": "^0.21.2",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-bootstrap": "^1.6.0",
    "react-bootstrap-icons": "^1.5.0",
    "react-dom": "17.0.1",
    "react-resizable": "^3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.2.1"
  },
  "bit": {
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn",
    "overrides": {
      "*": {
        "dependencies": {
          "prop-types": "-",
          "react-bootstrap": "-",
          "react-resizable": "-",
          "react": "-",
          "file-saver": "-",
          "jszip": "-",
          "@monaco-editor/react": "-"
        },
        "peerDependencies": {
          "prop-types": "+",
          "react-bootstrap": "+",
          "react-resizable": "+",
          "react": "+",
          "file-saver": "+",
          "jszip": "+",
          "@monaco-editor/react": "+"
        }
      }
    }
  }
}
