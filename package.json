{
  "name": "vanillamod-transpiler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "lint": "eslint components/**/*.js && eslint src/**/*.js",
    "pretty": "prettier --write components/ && prettier --write src/",
    "udeploy": "GIT_USER=$(grep GIT_USER .env | cut -d '=' -f2) docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/react": "^1.6.21",
    "@monaco-editor/react": "^4.0.9",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.1.11",
    "esprima": "^4.0.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.5.0",
    "monaco-editor": "^0.21.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-resizable": "^3.0.1",
    "react-split-pane": "^2.0.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.2.1"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@2.0.0"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn"
  }
}
