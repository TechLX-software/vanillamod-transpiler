{
  "drone-summon": {
    "name": "drone-summon",
    "type": "mcfunction",
    "contents": [
      "execute as @p[y_rotation=-45..45] run tp @e[tag=VanillaMod-newDroneReferencer] ~ ~ ~ 0 0",
      "execute as @p[y_rotation=45..135] run tp @e[tag=VanillaMod-newDroneReferencer] ~ ~ ~ 90 0",
      "execute as @p[y_rotation=135..225] run tp @e[tag=VanillaMod-newDroneReferencer] ~ ~ ~ 180 0",
      "execute as @p[y_rotation=225..315] run tp @e[tag=VanillaMod-newDroneReferencer] ~ ~ ~ 270 0",
      "tag @e[tag=VanillaMod-newDroneReferencer] remove VanillaMod-newDroneReferencer"
    ]
  }
}